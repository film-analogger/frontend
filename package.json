{
    "name": "@film-analogger/frontend",
    "version": "0.0.0",
    "packageManager": "yarn@4.12.0",
    "type": "module",
    "scripts": {
        "build": "react-router build",
        "dev": "react-router dev",
        "start": "react-router-serve ./build/server/index.js",
        "typecheck": "tsc --noEmit",
        "typegen": "react-router typegen && tsc",
        "lint": "eslint -c eslint.config.mts --fix --cache",
        "lint:nocache": "eslint -c eslint.config.mts --fix",
        "lint:ci": "eslint -c eslint.config.mts -f json -o reports/lint.json",
        "prepare": "husky",
        "test": "vitest",
        "test:coverage": "vitest run --coverage",
        "pw": "playwright test",
        "pw:ui": "playwright test --ui",
        "pw:report": "playwright show-report",
        "pw:headed": "playwright test --headed",
        "i18n:check": "i18n-check --locales src/i18n/locales -s en -f i18next"
    },
    "lint-staged": {
        "*": "yarn lint"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@fontsource/roboto": "^5.2.9",
        "@mui/icons-material": "^7.3.8",
        "@mui/material": "^7.3.8",
        "@react-router/node": "7.13.0",
        "@react-router/serve": "7.13.0",
        "i18next": "^25.8.13",
        "isbot": "^5.1.35",
        "react": "^19.2.4",
        "react-dom": "^19.2.4",
        "react-i18next": "^16.5.4",
        "react-router": "7.13.0"
    },
    "devDependencies": {
        "@eslint/compat": "^2.0.2",
        "@eslint/eslintrc": "^3.3.3",
        "@eslint/js": "^10.0.1",
        "@lingual/i18n-check": "^0.8.19",
        "@playwright/test": "^1.58.2",
        "@react-router/dev": "^7.13.0",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.2",
        "@types/eslint-plugin-jsx-a11y": "^6.10.1",
        "@types/node": "^22.19.11",
        "@types/react": "^19.2.14",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.4",
        "@vitest/coverage-v8": "^4.0.18",
        "@vitest/eslint-plugin": "^1.6.9",
        "eslint": "^10.0.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-check-file": "^3.3.1",
        "eslint-plugin-filename-export": "^2.0.0",
        "eslint-plugin-i18next": "^6.1.3",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-prettier": "^5.5.5",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-naming-convention": "^2.13.0",
        "eslint-plugin-react-refresh": "^0.5.0",
        "globals": "^17.3.0",
        "husky": "^9.1.7",
        "i18next-browser-languagedetector": "^8.2.1",
        "i18next-http-backend": "^3.0.2",
        "jiti": "^2.6.1",
        "jsdom": "^28.1.0",
        "lint-staged": "^16.2.7",
        "pinst": "^3.0.0",
        "prettier": "^3.8.1",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.56.0",
        "vite": "^7.3.1",
        "vite-plugin-eslint": "^1.8.1",
        "vite-tsconfig-paths": "^6.1.1",
        "vitest": "^4.0.18"
    }
}
